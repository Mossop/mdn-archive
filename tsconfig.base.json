{
  "compilerOptions": {
    "target": "es2021",
    "moduleResolution": "node",
    "sourceMap": true,
    "incremental": true,
    "esModuleInterop": true,
    "noEmitOnError": true,
    "outDir": "./dist",

    "noImplicitAny": false,
    "noImplicitThis": true,
    "noImplicitOverride": true,
    "strictPropertyInitialization": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true
  }
}
